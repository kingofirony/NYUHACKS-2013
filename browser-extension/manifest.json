{
  "manifest_version": 2,

  "name": "Reddit Analyzer",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",
  "options_page": "/html/options.html",

  "permissions": [
    "http://www.reddit.com/",
    "tabs",
     "<all_urls>",
     "storage" 
  ],

  "browser_action": {
    "default_icon": "/static/icon.png",
    "default_popup": "/html/popup.html"
  }
}